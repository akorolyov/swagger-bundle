{
    "name" : "enneite/swagger-bundle",
    "type" : "symfony-bundle",
    "description" : "Symfony bundle generator from swagger interface (yml or json)",
    "licence" : "MIT",
    "authors": [
        {
            "name": "Bernard Caron",
            "email": "caronbernard1@gmail.com"
        }
    ],
    "autoload":     {
        "psr-4": { "Enneite\\SwaggerBundle\\": "" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/enneite/swagger-bundle.git"
        }
    ],
    "require" :  {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.4",
        "symfony/framework-bundle": "~2.4",
        "symfony/console": "~2.4",
        "symfony/yaml": "~2.4",
        "twig/twig": "~1.0",
        "twig/extensions": "~1.1",
        "symfony/twig-bridge" : "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2.0",
        "fabpot/php-cs-fixer": "^1.10"
    }
}