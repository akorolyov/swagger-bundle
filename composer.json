{
    "name" : "enneite/swagger-bundle",
    "type" : "symfony-bundle",
    "description" : "Symfony bundle generator from swagger interface (yml or json)",
    "keywords": ["swagger", "api", "rest", "symfony"],
    "licence" : "MIT",
    "authors": [
        {
            "name": "Bernard Caron",
            "email": "caronbernard1@gmail.com"
        },
        {
            "name": "Etienne Le jariel",
            "email": "pitlejariel@hotmail.com"
        }
    ],
    "autoload":     {
        "psr-4": { "Enneite\\SwaggerBundle\\": "" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/enneite/swagger-bundle.git"
        }
    ],
    "require" :  {
        "php": ">=5.3.3",
        "symfony/symfony": "dev-master",
        "symfony/framework-bundle": "dev-master",
        "symfony/console": "dev-master",
        "symfony/yaml": "dev-master",
        "twig/twig": "dev-master",
        "twig/extensions": "dev-master",
        "symfony/twig-bridge" : "dev-master",
        "fabpot/php-cs-fixer": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "dev-master"
    }
}
